{
  "compilerOptions": {
    "target": "es2020",
    "module": "ESNext",
    "sourceMap": true,
    "strict": true,
    "noUnusedLocals": true,
    "jsx": "preserve",
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": ".",
    "types": [
      "unplugin-vue-define-options/macros-global",
      "node",
      "vite/client"
    ],
    "typeRoots": ["."],
    "paths": {
      "@/*": ["src/*"]
    }
  }
}
